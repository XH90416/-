<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.trcode.api.dao.TbCityDao">
    <select id="searchCityCode" parameterType="String" resultType="String">
        SELECT code
        FROM tb_city
        WHERE city = #{city}
    </select>
</mapper>